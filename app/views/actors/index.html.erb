<h1>List of Actors</h1>

<a href="http://localhost:3000/actor/new">Add a New Actor</a>

<table>
  <% @list_of_actors.each do |actor| %>
  <tr>
    <td>
      <img src="<%= actor.image_url %>" width="80">
    </td>
    <td>
      <%= actor.name %>
    </td>
    <td>
      <a href="http://localhost:3000/actors/<%= actor.id %>">full bio</a>
    </td>
    <td>
      <a href="http://localhost:3000/actors/<%= actor.id %>/edit">Edit</a>
    </td>
    <td>
      <a href="http://localhost:3000/delete_actors/<%= actor.id %>">Delete</a>
    </td>
  </tr>
  <% end %>
</table>
