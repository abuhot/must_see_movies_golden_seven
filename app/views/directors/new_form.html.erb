<form action="http://localhost:3000/create_director">
  <div>
    <label for="director_name">Director Name:</label>
    <input id="director_name" type="text" name="name">
  </div>
  <div>
    <label for="bio">Bio:</label>
    <input id="bio" type="text" name="bio">
  </div>
  <div>
    <label for="dob">Date of Birth:</label>
    <input id="dob" type="text" name="dob">
  </div>
