<form action="http://localhost:3000/create_movie">
  <div>
    <label for="movie_title">Movie Title:</label>
    <input id="movie_title" type="text" name="title">
  </div>
  <div>
    <label for="year">Year:</label>
    <input id="year" type="text" name="year">
  </div>
  <div>
    <label for="duration">Duration:</label>
    <input id="duration" type="text" name="duration">
  </div>
  <div>
    <label for="description">Description:</label>
    <input id="description" type="text" name="description">
  </div>
  <div>
    <label for="photo_image_url">Image URL:</label>
    <input id="photo_image_url" type="text" name="image_url">
  </div>
  <div>
    <button>Create Movie</button>
  </div>
</form>
